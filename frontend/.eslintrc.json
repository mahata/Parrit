{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true,
    "jasmine": true
  },
  "ignorePatterns": ["dist/"],
  "rules": {
    "react/prop-types": "off",
    "no-unused-vars": "warn",
    "no-redeclare": "warn",
    "no-undef": "warn"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
